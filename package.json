{
  "name": "@./source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "pnpm run dev:frontend",
    "dev:frontend": "nx serve frontend",
    "dev:backend": "nx serve backend",
    "dev:all": "nx run-many -t serve",
    "build": "nx run-many -t build",
    "build:frontend": "nx build frontend",
    "build:backend": "nx build backend",
    "test": "nx run-many -t test",
    "test:shared": "nx test shared-utils",
    "test:frontend": "nx test frontend",
    "test:backend": "nx test backend",
    "test:coverage": "pnpm run test:coverage:shared && pnpm run test:coverage:frontend && pnpm run test:coverage:backend",
    "test:coverage:shared": "nx test shared-utils --coverage",
    "test:coverage:frontend": "nx test frontend --coverage",
    "test:coverage:backend": "nx test backend --coverage",
    "coverage:view": "pnpm run coverage:view:shared && pnpm run coverage:view:frontend && pnpm run coverage:view:backend",
    "coverage:view:shared": "open coverage/libs/shared-utils/index.html",
    "coverage:view:frontend": "open coverage/apps/frontend/index.html",
    "coverage:view:backend": "open coverage/apps/backend/index.html",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "typecheck": "nx run-many -t typecheck",
    "clean": "nx reset",
    "graph": "nx graph",
    "affected:build": "nx affected -t build",
    "affected:test": "nx affected -t test",
    "affected:lint": "nx affected -t lint"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "21.2.3",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "21.2.3",
    "@nx/express": "^21.2.3",
    "@nx/jest": "21.2.3",
    "@nx/js": "21.2.3",
    "@nx/node": "^21.2.3",
    "@nx/react": "^21.2.3",
    "@nx/vite": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/webpack": "21.2.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.2.3",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0"
  }
}
